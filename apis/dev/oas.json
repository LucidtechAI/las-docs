  "components": {
    "schemas": {
      "PostBatches": {
        "title": "POST /batches",
        "required": [
          "description"
        ],
        "type": "object",
        "properties": {
          "description": {
            "minLength": 1,
            "type": "string"
          }
        },
        "additionalProperties": false
      },
      "User": {
        "title": "user",
        "type": "object"
      },
      "Prediction": {
        "title": "prediction",
        "required": [
          "documentId",
          "predictions"
        ],
        "type": "object",
        "properties": {
          "documentId": {
            "maxLength": 36,
            "minLength": 36,
            "pattern": "^[a-f0-9-]+$",
            "type": "string"
          },
          "predictions": {
            "minItems": 1,
            "type": "array",
            "items": {
              "required": [
                "confidence",
                "label",
                "value"
              ],
              "type": "object",
              "properties": {
                "confidence": {
                  "maximum": 1,
                  "minimum": 0,
                  "type": "number"
                },
                "label": {
                  "maxLength": 36,
                  "minLength": 1,
                  "pattern": "^[A-Za-z_]+$",
                  "type": "string"
                },
                "value": {
                  "anyOf": [
                    {
                      "maxLength": 64,
                      "minLength": 1,
                      "type": "string"
                    },
                    null
                  ]
                }
              },
              "additionalProperties": false
            }
          }
        },
        "additionalProperties": false
      },
      "Error": {
        "title": "Error Schema",
        "type": "object",
        "properties": {
          "message": {
            "type": "string"
          }
        }
      },
      "Document": {
        "title": "document",
        "oneOf": [
          {
            "required": [
              "consentId",
              "contentType",
              "documentId"
            ],
            "type": "object",
            "properties": {
              "feedback": {
                "minItems": 1,
                "type": "array",
                "items": {
                  "required": [
                    "label",
                    "value"
                  ],
                  "type": "object",
                  "properties": {
                    "label": {
                      "maxLength": 36,
                      "minLength": 1,
                      "pattern": "^[A-Za-z_]+$",
                      "type": "string"
                    },
                    "value": {
                      "anyOf": [
                        {
                          "maxLength": 64,
                          "minLength": 1,
                          "type": "string"
                        },
                        null
                      ]
                    }
                  },
                  "additionalProperties": false
                }
              },
              "consentId": {
                "maxLength": 64,
                "minLength": 1,
                "pattern": "^[A-Za-z0-9_-]+$",
                "type": "string"
              },
              "inferenceTime": {
                "minimum": 0,
                "type": "number"
              },
              "documentId": {
                "maxLength": 36,
                "minLength": 36,
                "pattern": "^[a-f0-9-]+$",
                "type": "string"
              },
              "batchId": {
                "maxLength": 36,
                "minLength": 36,
                "pattern": "^[a-f0-9-]+$",
                "type": "string"
              },
              "contentType": {
                "type": "string",
                "enum": [
                  "application/pdf",
                  "image/jpeg"
                ]
              },
              "updated": {
                "minimum": 1,
